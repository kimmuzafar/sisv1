{% block pagestylesdown %}
    <style>
	    /* demo page styles */
	    .custom-nav-animation li {
	        display: none;
	    }
	    .custom-nav-animation li.animated {
	        display: block;
	    }
		.email-template {
			display: none;
		}
		.email-template.active {
